CREATE TABLE IF NOT EXISTS aire (
    Estacion TEXT,
    Titulo TEXT,
    Latitud TEXT,
    Longitud TEXT,
    Fecha TEXT,
    Periodo TEXT,
    SO2 TEXT,
    NO TEXT,
    NO2 TEXT,
    CO TEXT,
    PM10 TEXT,
    O3 TEXT,
    DD TEXT,
    VV TEXT,
    TMP TEXT,
    HR TEXT,
    PRB TEXT,
    RS TEXT,
    LL TEXT,
    BEN TEXT,
    TOL TEXT,
    MXIL TEXT,
    PM25 TEXT,
    PRIMARY KEY (Estacion, Fecha, Periodo)
);
